    <validationRules>
        <fullName>End_Data_On_Insert</fullName>
        <active>true</active>
        <description>You can&apos;t provide end data (status/date) when creating new Nuzlocke</description>
        <errorConditionFormula>AND(
 ISNEW(),
 OR(
  NOT(ISBLANK(TEXT(End_Status__c))),
  NOT(ISBLANK(End_Date__c))
 )
)</errorConditionFormula>
        <errorMessage>You can&apos;t provide end data (status/date) when creating new Nuzlocke</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Before_Start_Date</fullName>
        <active>true</active>
        <description>End Date can&apos;t be before Start Date</description>
        <errorConditionFormula>End_Date__c &lt; Start_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End Date can&apos;t be before Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Full_End_Data_Provided</fullName>
        <active>true</active>
        <description>When finishing the Nuzlocke you have to provide both End Date and End Status</description>
        <errorConditionFormula>AND(
 NOT(ISNEW()),
 OR(
  AND(
   NOT(ISBLANK(End_Date__c)),
   ISBLANK(TEXT(End_Status__c))
  ),
  AND(
   NOT(ISBLANK(TEXT(End_Status__c))),
   ISBLANK(End_Date__c)
  )
 )
)</errorConditionFormula>
        <errorMessage>When finishing the Nuzlocke you have to provide both End Date and End Status</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Nuzlocke_Finished</fullName>
        <active>true</active>
        <description>Can&apos;t finish Nuzlocke that is already finished</description>
        <errorConditionFormula>AND(
 PRIORVALUE(Is_Finished__c),
 OR(
  AND(
   ISCHANGED(End_Status__c),
   NOT(ISBLANK(TEXT(End_Status__c)))
  ),
  AND(
   ISCHANGED(End_Date__c),
   NOT(ISBLANK(End_Date__c))
  )
 )
)</errorConditionFormula>
        <errorMessage>Can&apos;t finish Nuzlocke that is already finished</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
