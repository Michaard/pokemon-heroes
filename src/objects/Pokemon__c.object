    <validationRules>
        <fullName>Death_Info_While_Alive</fullName>
        <active>true</active>
        <description>You can&apos;t provide death information while Pokemon is not dead</description>
        <errorConditionFormula>AND(
 Death__c = false,
 OR(
  NOT(ISBLANK(Death_Level__c)),
  NOT(ISBLANK(Cause_of_Death__c))
 )
)</errorConditionFormula>
        <errorMessage>You can&apos;t provide death information while Pokemon is not dead</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Forbidden_Alolan_Form</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Is_Alolan__c = true, Form__r.Has_Alolan_Variant__c = false)</errorConditionFormula>
        <errorDisplayField>Is_Alolan__c</errorDisplayField>
        <errorMessage>The pokemon form doesn&apos;t have alolan variant</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Form_Wrong_Family</fullName>
        <active>true</active>
        <errorConditionFormula>TEXT(Caught_Form__r.Family__c) &lt;&gt; TEXT(Form__r.Family__c)</errorConditionFormula>
        <errorDisplayField>Form__c</errorDisplayField>
        <errorMessage>The pokemon form must be related to the caught pokemon</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Form_Wrong_Stage</fullName>
        <active>true</active>
        <errorConditionFormula>Caught_Form__r.Family_Stage__c &gt; Form__r.Family_Stage__c</errorConditionFormula>
        <errorDisplayField>Form__c</errorDisplayField>
        <errorMessage>The pokemon form stage cannot be lower than caught form stage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Invalid_Death_Level</fullName>
        <active>true</active>
        <description>Deth level must be in range 1-100</description>
        <errorConditionFormula>AND(
 Death__c = true,
 OR(
  Death_Level__c &lt; 1,
  Death_Level__c &gt; 100
 )
)</errorConditionFormula>
        <errorDisplayField>Death_Level__c</errorDisplayField>
        <errorMessage>Deth level must be in range 1-100</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Nuzlocke_Finished</fullName>
        <active>true</active>
        <description>You can&apos;t add new Pokemon to finished Nuzlocke</description>
        <errorConditionFormula>AND(
 ISNEW(),
 Nuzlocke__r.Is_Finished__c
)</errorConditionFormula>
        <errorMessage>You can&apos;t add new Pokemon to finished Nuzlocke</errorMessage>
    </validationRules>
