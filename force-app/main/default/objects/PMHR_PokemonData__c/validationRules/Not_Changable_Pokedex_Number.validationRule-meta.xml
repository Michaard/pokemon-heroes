<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Not_Changable_Pokedex_Number</fullName>
    <active>true</active>
    <description>Pokedex Number cannot be edited</description>
    <errorConditionFormula>AND(
 NOT(ISNEW()),
 ISCHANGED(PokedexNumber__c)
)</errorConditionFormula>
    <errorDisplayField>PokedexNumber__c</errorDisplayField>
    <errorMessage>Pokedex Number cannot be edited</errorMessage>
</ValidationRule>
