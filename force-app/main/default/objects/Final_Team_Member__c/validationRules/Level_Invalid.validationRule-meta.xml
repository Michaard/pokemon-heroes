<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Level_Invalid</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Level__c &lt; 1, Level__c &gt; 100)</errorConditionFormula>
    <errorDisplayField>Level__c</errorDisplayField>
    <errorMessage>Level must be between 1 and 100</errorMessage>
</ValidationRule>
